{
    "env": {
        "sharedIncludePath": [
            "${workspaceFolder}/**",
            "${workspaceFolder}/terra/**"
        ],
        "sharedDefines": [
            "DEBUG"
        ]
    },
    "configurations": [
        {
            "name": "Emulator",
            "includePath": [
                "${sharedIncludePath}"
            ],
            "defines": [
                "${sharedDefines}"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "clang-x64",
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${sharedIncludePath}"
                ]
            }
        },
        {
            "name": "ArduinoUnoWifiRev2",
            "includePath": [
                "${sharedIncludePath}"
            ],
            "defines": [
                "${sharedDefines}",
                "ARDUINO=108013",
                "UNO_WIFI_REV2_328MODE",
                "ARDUINO_ARCH_NRF52",
                "USBCON"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "clang-x64",
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${sharedIncludePath}"
                ]
            }
        }
    ],
    "version": 4
}